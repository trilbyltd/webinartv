<%= simple_form_for([:admin, @webinar]) do |f| %>
  <%= f.error_notification %>

  <div class="form-input">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :live_date %>
    <%= f.collection_select(:presenter_id, Presenter.all, :id, :name) %>
    <%= f.input :webinar_url %>
    <%= f.input :active %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
