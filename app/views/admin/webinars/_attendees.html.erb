<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>School/Company</th>
    <th>Attended?</th>
  </tr>
  <% @webinar.attendees.each do |attendee| %>
    <tr>
      <td><%= attendee.name %></td>
      <td><%= attendee.email %>&nbsp;<%= link_to image_tag('email.jpg'), "mailto:#{attendee.email}" %></td>
      <td><%= attendee.school_name %></td>
      <td><%= attendee.attended?(@webinar) %></td>
    </tr>
    <% end %>
  </table>
