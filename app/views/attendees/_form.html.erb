<%= simple_form_for(@attendee) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, input_html: { autocomplete: 'name-full'} %>
    <%= f.input :email, input_html: { autocomplete: 'email' } %>
    <%= f.input :active_user %>
    <%= f.input :school_name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
