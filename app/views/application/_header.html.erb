  <header>
    <nav>
      
      <ul class="left">
      <li class="nav-link"><%= link_to "TrilbyTV Webinars", root_path %>
      <% if signed_in? %>
        <% if current_user.admin? %>
          <li class="nav-link">
            <%= link_to admin_webinars_path do %>
              <% t(".webinars") %>
            <% end %>
          <li class="nav-link"><%= link_to t(".attendees"), attendees_path %>
          <li class="nav-link"><%= link_to t(".presenters"), presenters_path %>
        <% end %>
        <li class="nav-link"><%= link_to t("layouts.application.sign_out"), sign_out_path, method: :delete %>
      <% else %>
        <li class="nav-link"><%= link_to t("layouts.application.sign_in"), sign_in_path %>
      <% end %>
      </ul>
    </nav>
  </header>
